<%- include('./partials/header') %>
<div>
  <h3>Pick Your Team</h3>
</div>
<table>
    <thead>
      <tr>
        <th>Team</th>
        <th>Bye Week</th>

      </tr>
    </thead>
    <tbody>
      <% teams.forEach(function(m) { %>
        <tr>
          <td><%= m.name %></td>
          <td><%= m.byeWeek %></td>
          <td><a href="/teams/<%=m._id %>">DETAILS</a></td>
        </tr>
      <% }); %>
    </tbody>
  </table>
<input type="submit" value="Select Team">
<form action="/teams/new">
<input type="submit" value="Add Team">
</form>




<%- include('./partials/footer') %> 